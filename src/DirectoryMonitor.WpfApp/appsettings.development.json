{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "NLog": {
        "internalLogLevel": "Debug",
        "rules": [
            {
                "ruleName": "debug",
                "enabled": true,
                "logger": "*"
            },
            {
                "ruleName": "error",
                "enabled": true,
                "logger": "*"
            },
            {
                "ruleName": "logFile",
                "enabled": true,
                "logger": "*",
                "minLevel": "Debug"
            }
        ]
    }
}